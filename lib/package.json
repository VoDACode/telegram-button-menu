{
  "name": "telegram-button-menu",
  "version": "0.0.3",
  "description": "Makes it easier to create a button menu in Telegram bots",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./command": {
      "import": "./dist/commands.js",
      "default": "./dist/commands.js"
    },
    "./menu": {
      "import": "./dist/menu.js",
      "default": "./dist/menu.js"
    },
    "./handlers": {
      "import": "./dist/handlers.js",
      "default": "./dist/handlers.js"
    },
    "./translate": {
      "import": "./dist/translate.js",
      "default": "./dist/translate.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "pub": "npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VoDACode/telegram-button-menu.git"
  },
  "bugs": {
    "url": "https://github.com/VoDACode/telegram-button-menu/issues"
  },
  "homepage": "https://github.com/VoDACode/telegram-button-menu",
  "keywords": [
    "telegram",
    "bot",
    "button",
    "menu",
    "key",
    "keyboard"
  ],
  "author": "VoDACode",
  "license": "Apache 2.0",
  "dependencies": {
    "telegraf": "^4.16.3"
  },
  "devDependencies": {
    "@types/node": "^22.13.14"
  }
}
